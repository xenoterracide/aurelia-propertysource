{
  "name": "aurelia-propertysource",
  "version": "0.1.0",
  "description": "Spring Like PropertySource for Aurelia 2",
  "main": "index.js",
  "repository": "https://github.com/xenoterracide/aurelia-propertysource",
  "author": "Caleb Cushing <xenoterracide@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc --build",
    "test": "npx nyc ava --color test/**.ts",
    "clean": "npx rimraf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@aurelia/kernel": "^0.7.0-dev.202004080424",
    "@aurelia/metadata": "^0.7.0-dev.202004080424",
    "jsonpath-plus": "^3.0.0"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "ava": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "esm": "^3.2.25",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "ava": {
    "color": true,
    "extensions": [
      "ts"
    ],
    "require": [
      "source-map-support/register",
      "ts-node/register",
      "esm"
    ]
  }
}
